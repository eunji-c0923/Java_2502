package ch2.sec06;

public class SpringExample {

	public static void main(String[] args) {

		String name = "나는 자바\t를\t 배웁니다.";
				
		System.out.println(name);
		
		System.out.println("나는 자바를\n 배웁니다.");
		
		System.out.println("나는 \"자\"바를 배웁니다.");
		
		System.out.println("올해 처음이자\n마지막\\눈송이\t 네일이야");
	}

}
