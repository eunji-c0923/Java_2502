package ch02.sec03;

public class VariadleExam {

	public static void main(String[] args)  {
			
	int a = 10;
	int b = 20;
		
		System.out.println("before : ");
	    System.out.println("a = " + a +", " + "b = " + b);

			int c = a;
	        a = b;
	        b = c;
	        
	        System.out.println("after : ");
	        System.out.println("a = " + a +", " + "b = " + b);
	        
			

		
	
	 
	}
	

}
