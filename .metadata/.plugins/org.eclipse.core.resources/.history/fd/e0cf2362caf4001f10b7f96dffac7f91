package ch01.ex6;

public class OverflowExample {

	public static void main(String[] args) {

		short num = 32767;  //최대값
		short num = -32768;
	
		System.out.println(num1);
		System.out.println(num2);
		
		num1++;  //num = num +1
		
		System.out.println(num1);
		
		num2--;
        System.out.println(num2);		
		
		
		
	}

}
