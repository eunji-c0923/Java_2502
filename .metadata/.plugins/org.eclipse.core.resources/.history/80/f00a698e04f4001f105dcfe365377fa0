package ch2.sec01;

public class booleanExample {

	public static void main(String[] args) {

		boolean stop = true;
		
		stop = false;
		
		
		/*
		 * > : 크다
		 * >= : 크거나 같다
		 * < : 작다
		 * <= : 작거나 같다
		 * == : 같다
		 * != : 같지않다
		 * ! : 부정
		 * 
		 */
		
		boolean check = 15 != 5;
		System.out.println(check);
	}

}
