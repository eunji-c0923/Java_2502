package ch02.sec03;

public class VariadleExam {

	public static void main(String[] args)  {
			
	int a = 10;
	int b = 20;
	int c = 30;
	int d = 40;
	
		System.out.println("before : ");
	    System.out.println("a = " + a +", " + "b = " + b + "c = " + c +"," + "d = "+ d);

		    a = b;
	        b = c;
	        c = d;
	        d = c;
	        
	        System.out.println("after : ");
	        System.out.println("a = " + a +", " + "b = " + b + "c = " + c +", " + "d = "+ d);
	        
	        
	        //double > float > long > int > short > byte
	        
	        // fpoat(4) < double(8)
	        
	     
	        
			

		
	
	 
	}
	

}
