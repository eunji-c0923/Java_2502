package ch02.sec03;

public class VariadleExam {

	public static void main(String[] args)  {
			
	int a = 10;
	int b = 20;
	int c = 30;
	
		System.out.println("before : ");
	    System.out.println("a = " + a +", " + "b = " + b + "c =" + c);

			int c = a;
	        a = b;
	        b = c;
	        
	        System.out.println("after : ");
	        System.out.println("a = " + a +", " + "b = " + b +"c ="+ c);
	        
	        
	        //double > float > long > int > short > byte
	        
	        // fpoat(4) < double(8)
	        
	        float f1 = 10.0f
	        		
	        		byte b2 = 10;
	        
	        double d1 = 10.0;
	        int a = 10;
	        int b = 20;
	        
	        long l1 = 1000;
	        
	        f1 =l1;
	        
			

		
	
	 
	}
	

}
